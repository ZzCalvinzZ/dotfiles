{
  // could be 'always' 'trigger' => for specified trigger characters only 'none'
  "suggest.autoTrigger": "always",
  // only used when autoTrigger is always
  "suggest.triggerAfterInsertEnter": false,
  // timeout for completion
  "suggest.timeout": 500,
  // not make vim select first item on completion start
  "suggest.noselect": false,
  // executable path for https://facebook.github.io/watchman/, detected from $PATH by default
  // "coc.preferences.watchmanPath": "",
  //"coc.preferences.formatOnSaveFiletypes": ["python"]
  // enable diagnostic
  "diagnostic.enable": true,
  "diagnostic.signOffset": 1000,
  "diagnostic.errorSign": "✗",
  "diagnostic.warningSign": "?",
  "diagnostic.infoSign": "i",
  "diagnostic.hintSign": "h",
  "diagnostic.virtualText": true,
  "prettier.eslintIntegration": true,
  "python.jediEnabled": false,
  "python.formatting.provider": "black",
  "python.venvPath": "~/Envs",
  "python.linting.flake8Enabled": true,
  "git.changedSign.text": "~",
  "git.changedSign.hlGroup": "ModeMsg",
  "git.addedSign.text": "+",
  "git.addedSign.hlGroup": "Title",
  "git.removedSign.text": "_",
  "git.removedSign.hlGroup": "WarningMsg",
  "git.topRemovedSign.text": "‾",
  "git.topRemovedSign.hlGroup": "WarningMsg",
  "git.changeRemovedSign.text": "≃",
  "git.changeRemovedSign.hlGroup": "ModeMsg",
  "codelens.enable": true
}
