{
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"parser": "babel-eslint",
	"plugins": ["react", "babel"],
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		}
	},
	"rules": {
		"strict": 0,
		"babel/semi": 1,
		"react/jsx-uses-vars": 1,
		"react/react-in-jsx-scope": 1,
		"react/prop-types": 0,
		"prettier/prettier": "error"
	},
	"globals": {
		"gettext": true,
		"$": true,
		"dragula": true,
		"reverse": true,
		"React": true,
		"R": true,
		"_": true
	}
}
