{
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"parser": "babel-eslint",
	"plugins": ["react"],
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		}
	},
	"rules": {
		"strict": 0,
		"semi": 1,
		"babel/semi": 1,
		"react/jsx-uses-vars": 1,
		"react/react-in-jsx-scope": 1
	},
	"globals": {
		"gettext": true
	}

}
