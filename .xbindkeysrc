# Increase volume
"amixer set Master 5%+"
   XF86AudioRaiseVolume

# Decrease volume
"amixer set Master 5%-"
   XF86AudioLowerVolume

# Mute volume
"amixer set Master toggle"
   XF86AudioMute

"playerctl play-pause"
    Control+Alt + Return

"playerctl play-pause"
    XF86AudioPlay

"playerctl next"
    XF86AudioNext

"playerctl previous"
    XF86AudioPrev

"xbacklight -inc 10"
    XF86MonBrightnessUp

"xbacklight -dec 10"
    XF86MonBrightnessDown
